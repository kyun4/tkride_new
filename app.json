{
  "expo": {
    "name": "TK Ride",
    "slug": "tkride_mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tkridelogo2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tkridelogo2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tiyokanor32.tkride_mobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "76dd6ee2-db27-4238-92c0-d8a5a273500d"
      }
    }
  }
}
